{
  "expo": {
    "name": "lexo",
    "slug": "lexo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "lexo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "com.ardacey.lexo",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "environment": "development",
      "apiUrl": "https://lexo-production.up.railway.app",
      "wsUrl": "wss://lexo-production.up.railway.app",
      "clerkPublishableKey": "pk_live_ZnVsbC1zZWFndWxsLTgzLmNsZXJrLmFjY291bnRzLmRldiQ",
      "router": {},
      "eas": {
        "projectId": "04e02cd6-3815-4543-889c-0dd47b7bb2b5"
      }
    },
    "owner": "ardacey"
  }
}
